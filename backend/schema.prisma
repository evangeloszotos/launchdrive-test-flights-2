datasource postgresql {
  url      = env("DATABASE_URL")
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
  output   = "node_modules/.prisma/client"
}

model User {
  id       String @id @default(cuid())
  name     String @default("")
  email    String @unique @default("")
  password String
  s3Secret String @default("")

  @@index([s3Secret])
}

model TestFlight {
  id       String              @id @default(cuid())
  name     String              @default("")
  variants TestFlightVariant[] @relation("TestFlightVariant_testFlight")
}

model TestFlightVariant {
  id                     String      @id @default(cuid())
  name                   String      @default("")
  testFlight             TestFlight? @relation("TestFlightVariant_testFlight", fields: [testFlightId], references: [id])
  testFlightId           String?     @map("testFlight")
  from_TextInput_variant TextInput[] @relation("TextInput_variant")

  @@index([testFlightId])
}

model TextInput {
  id           String             @id @default(cuid())
  variant      TestFlightVariant? @relation("TextInput_variant", fields: [variantId], references: [id])
  variantId    String?            @map("variant")
  inputFieldId String             @default("")
  value        String             @default("")

  @@index([variantId])
}